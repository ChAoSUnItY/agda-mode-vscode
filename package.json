				"mac": "ctrl+c ctrl+w",
				"when": "agdaMode"
			},
			{
				"command": "agda-mode.escape",
				"key": "escape",
				"mac": "escape",
				"when": "agdaMode && agdaModeQuerying || agdaModeTyping"
			},
			{
				"command": "agda-mode.input-symbol[MoveUp]",
				"key": "up",
				"when": "agdaMode && agdaModeTyping"
