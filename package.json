{
	"name": "agda-mode",
	"displayName": "agda-mode",
	"description": "agda-mode on vscode",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.load"
	],
	"main": "./lib/js/src/Main.bs.js",
	"contributes": {
		"languages": [
			{
				"id": "agda",
				"extensions": [
					".agda"
				],
				"aliases": [
					"Agda"
				],
				"filenames": []
			}
		],
		"commands": [
			{
				"command": "extension.load",
				"title": "Load"
			}
		],
		"keybindings": [
			{
				"command": "extension.load",
				"key": "ctrl+c ctrl+l",
				"mac": "ctrl+c ctrl+l"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js",
		"build": "bsb -make-world",
		"start": "bsb -make-world -w",
		"clean": "bsb -clean-world"
	},
	"devDependencies": {
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"agda-mode": "github:banacorn/agda-mode#dev",
		"bs-platform": "^7.0.1",
		"reason-promise": "^1.0.1"
	}
}
