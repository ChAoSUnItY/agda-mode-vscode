{
	"name": "agda-mode",
	"displayName": "agda-mode",
	"description": "agda-mode on vscode",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld",
		"onCommand:extension.hey"
	],
	"main": "./lib/js/src/Main.bs.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "extension.hey",
				"title": "Hey"
			}
		],
		"keybindings": [
			{
				"command": "extension.helloWorld",
				"key": "ctrl+c ctrl+l",
				"mac": "ctrl+c ctrl+l"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js",
		"build": "bsb -make-world",
		"start": "bsb -make-world -w",
		"clean": "bsb -clean-world"
	},
	"devDependencies": {
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"vscode-test": "^1.2.2",
		"@glennsl/bs-json": "^3.0.0",
		"bs-platform": "^7.0.1",
		"bs-webapi": "^0.15.3"
	},
	"dependencies": {
		"react": "^16.9.0",
		"react-dom": "^16.9.0",
		"reason-react": "^0.7.0",
		"reason-react-update": "^0.1.1"
	}
}